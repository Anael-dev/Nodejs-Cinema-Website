<div class="flex">
<%- include ./menuOptions%>
</div>
<div class="all">
    <% subscriptions.forEach((x)=>{ %>
  <div class="element">
    <ul>
    <li><h3><%=x.name%></h3></li>
    <li><label>Email:</label><%=x.email%></li>
    <li><label>City:</label><%=x.city%></li>
    <% if(user.permissions.includes("Delete Subscriptions"|| "Update Subscriptions")){ %>
      <li><%- include ./buttonOptions%> </li>
      <% } %>


  </ul>

  </div>
    <% }) %>
</div>